    "@types/pump": "^1.1.0",
    "@types/split2": "^2.1.6",
    "@types/through2": "^2.0.34",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
