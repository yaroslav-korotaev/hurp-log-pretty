  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "hurp-log-pretty": "./bin/cli.js"
  },
  "repository": "yaroslav-korotaev/hurp-log-pretty",
  "scripts": {
